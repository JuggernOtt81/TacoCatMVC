<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - TacoCatMVC</title>
    @*<link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />*@
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-F3w7mX95PdgyTmZZMECAngseQB83DfGTowi0iMjiWaeVhAn4FJkqJByhZMI3AhiU" crossorigin="anonymous" />
          
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/css/font-awesome-6/css/all/css" />
    <link rel="stylesheet" href="~/css/font-awesome-6/webfonts/" />
    <link rel="stylesheet" href="~/css/sitewide-page-template-base.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/TacoCatMVC.styles.css" asp-append-version="true" />
</head>
<body>
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3">
            <div class="container-fluid">
                <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index">TacoCatMVC</a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex justify-content-between">
                    <ul class="navbar-nav flex-grow-1">
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Reverse">Palindrome</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>
    </header>

    <main role="main" class="flex-shrink-0">
        @RenderBody()
    </main>


    <footer class="m-0 bg-dark footer">
        <div class="row container-fluid text-center">

            <div class="col-3 justify-content-start">
                <span class="footerspan ">
                    <a class="footerlink" href="https://www.juggernott81.com"> &copy; 2022</a>
                </span>
            </div>
            <!-- want to make this middle div col do something different on smaller screens, maybe cycle through the <a href="#"><i>icon</i>links</a> one at a time... but how? -->
            <div class="col-2">
            </div>
            <div class="col-2 justify-content-center">
                <div class="hwrap">
                    <div class="hmove">
                        <div class="hitem">
                            <a class="footerlink" href="https://github.com/JuggernOtt81"
                               target="_blank"><i class="fa-brands fa-github"></i></a>
                        </div>
                        <div class="hitem">
                            <a class="footerlink" href="https://www.linkedin.com/in/lawsonott/"
                               target="_blank"><i class="fa-brands fa-linkedin"></i></a>
                        </div>
                        <div class="hitem">
                            <a class="footerlink"
                               href="https://stackoverflow.com/users/9724160/juggern-ott" target="_blank">
                                <i class="fa-brands fa-stack-overflow"></i>
                            </a>
                        </div>
                        <div class="hitem">
                            <a class="footerlink" href="https://twitter.com/juggernott81"
                               target="_blank"><i class="fa-brands fa-twitter"></i></a>
                        </div>
                        <div class="hitem">
                            <a class="footerlink"
                               href="https://www.youtube.com/channel/UCo-xCEut1wxPv14RMFznbOg" target="_blank">
                                <i class="fa-brands fa-youtube"></i>
                            </a>
                        </div>
                        <div class="hitem">
                            <a class="footerlink" href="https://www.reddit.com/u/juggernott81"
                               target="_blank"><i class="fa-brands fa-reddit"></i></a>
                        </div>
                        <div class="hitem">
                            <a class="footerlink" href="https://www.quora.com/profile/Juggern-Ott"
                               target="_blank"><i class="fa-brands fa-quora"></i></a>
                        </div>
                        <div class="hitem">
                            <a class="footerlink" href="https://www.tiktok.com/juggernott81"
                               target="_blank"><i class="fa-brands fa-tiktok"></i></a>
                        </div>
                        <div class="hitem">
                            <a class="footerlink" href="https://codepen.io/JuggernOtt81/pen/gzqNmW"
                               target="_blank"><i class="fa-brands fa-codepen"></i></a>
                        </div>
                        <div class="hitem">
                            <a class="footerlink" href="https://www.freecodecamp.org/juggernott81"
                               target="_blank"><i class="fa-brands fa-free-code-camp"></i></a>
                        </div>
                        <div class="hitem">
                            <a class="footerlink"
                               href="https://play.google.com/store/apps/developer?id=JuggernOtt81&hl=en_US&gl=US"
                               target="_blank"><i class="fa-brands fa-google-play"></i></a>
                        </div>
                        <div class="hitem">
                            <a class="footerlink"
                               href="https://developer.apple.com/forums/profile/JuggernOtt81" target="_blank">
                                <i class="fa-brands fa-apple"></i>
                            </a>
                        </div>
                        <div class="hitem">
                            <a class="footerlink"
                               href="https://techcommunity.microsoft.com/t5/user/viewprofilepage/user-id/1305014#profile"
                               target="_blank"><i class="fa-brands fa-microsoft"></i></a>
                        </div>
                        <div class="hitem">
                            <a class="footerlink" href="https://youtu.be/yJ9glRHCLW8" target="_blank"
                               style="color:white"><i class="iconify" data-icon="mdi:rabbit"></i></a>
                        </div>
                        <div class="hitem"><a class="footerlink" href="#" target="_blank"></a></div>
                    </div>
                </div>
            </div>
            <div class="col-2">
            </div>
            <div class="col-3 justify-content-end">
                <span class="footerspan">
                    <a class="footerlink" href="https://github.com/JuggernOtt81/Showcase/blob/main/index.html"
                       target="_blank">view code</a>
                </span>
            </div>
        </div>

    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    @*<script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>*@
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.1/dist/js/bootstrap.bundle.min.js"
            integrity="sha384-/bQdsTh/da6pkI1MST/rWKFNjaCP5gBSY4sEBT38Q/9RBh9AH40zEOg7Hlq2THRZ" crossorigin="anonymous">
    </script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
